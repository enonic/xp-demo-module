<?xml version="1.0" encoding="UTF-8"?>
<content-type>
  <display-name>Event</display-name>
  <content-display-name-script>$('title')</content-display-name-script>
  <super-type>base:structured</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <is-built-in>false</is-built-in>
  <allow-child-content>true</allow-child-content>
  <form>
    <field-set name="basic">
      <label>Event</label>
      <items>
        <input type="TextLine" name="title">
          <label>Title</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="ImageSelector" name="image">
          <label>Image</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
          <config></config>
        </input>
        <input type="TextLine" name="linkText">
          <label>Link text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input type="TextLine" name="linkUrl">
          <label>Link url</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
    </field-set>
    <inline mixin="event-items"/>
  </form>
</content-type>