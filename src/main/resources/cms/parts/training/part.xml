<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Training and Certification</display-name>
  <config>
    <field-set name="info">
      <label>Info</label>
      <items>
        <input type="HtmlArea" name="text">
          <label>Text on the page</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="TextLine" name="url">
          <label>URL for link</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
    </field-set>
  </config>
</part-component>