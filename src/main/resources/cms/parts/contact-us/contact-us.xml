<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Contact us</display-name>
  <config>
    <field-set name="contact">
      <label>Contact</label>
      <items>
        <input type="TextLine" name="supportUrl">
          <label>Support URL</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="HtmlArea" name="text">
          <label>Text above form</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="HtmlArea" name="thankYou">
          <label>Thank you message</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="ContentSelector" name="saveFolder">
          <label>Save folder</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="0" maximum="1"/>
          <config>
            <relationship-type>system:reference</relationship-type>
            <allow-content-type>base:folder</allow-content-type>
          </config>
        </input>
      </items>
    </field-set>
  </config>
</part-component>
