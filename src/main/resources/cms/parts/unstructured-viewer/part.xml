<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Unstructured viewer</display-name>
  <config>
    <field-set name="data">
      <label>Config</label>
      <items>
        <input type="TextLine" name="numPerPage">
          <label>Number per page</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input type="ContentSelector" name="retrieveFolder">
          <label>Retrieve folder</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="0" maximum="1"/>
          <config>
            <relationship-type>system:reference</relationship-type>
            <allow-content-type>base:folder</allow-content-type>
          </config>
        </input>
      </items>
    </field-set>
  </config>
</part-component>