<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Event list</display-name>
  <config>
    <field-set name="events">
      <label>Events</label>
      <items>
        <input type="TextLine" name="numberEvents">
          <label>Number of Events</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input type="TextLine" name="title">
          <label>Title</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
    </field-set>
  </config>
</part-component>
