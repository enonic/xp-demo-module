<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part-component>
  <display-name>Try now video</display-name>
  <config>
    <field-set name="video-info">
      <label>Video info</label>
      <items>
        <input type="TextLine" name="heading">
          <label>Heading</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="HtmlArea" name="text">
          <label>Text</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="TextLine" name="video-id">
          <label>Video ID</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
    </field-set>
  </config>
</part-component>