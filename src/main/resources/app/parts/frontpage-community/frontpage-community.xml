<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
    <display-name>Frontpage community</display-name>
    <config>
        <input type="TextLine" name="title">
            <label>Title</label>
            <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="TextArea" name="preface">
            <label>Preface</label>
            <occurrences minimum="1" maximum="1"/>
        </input>
        <field-set name="link">
            <label>Link</label>
            <items>
                <inline mixin="link-url"/>
            </items>
        </field-set>
    </config>
</part>
