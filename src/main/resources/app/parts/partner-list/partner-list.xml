<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
    <display-name>Partner list</display-name>
    <config>
        <input type="HtmlArea" name="becomePartnerIntro">
            <label>Become partner intro</label>
            <immutable>false</immutable>
            <indexed>true</indexed>
            <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="HtmlArea" name="becomePartnerBody">
            <label>Become partner body</label>
            <immutable>false</immutable>
            <indexed>true</indexed>
            <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="ContentSelector" name="contactPage">
            <label>Contact page</label>
            <immutable>false</immutable>
            <indexed>true</indexed>
            <occurrences minimum="1" maximum="1"/>
            <config>
                <relationship-type>system:reference</relationship-type>
            </config>
        </input>
    </config>
</part>
