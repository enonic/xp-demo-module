<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
    <display-name>Powered by sites</display-name>
    <config>
        <input type="ContentSelector" name="contentLocation">
            <label>Content Location</label>
            <immutable>false</immutable>
            <indexed>true</indexed>
            <occurrences minimum="1" maximum="1"/>
            <config>
                <relationship-type>system:reference</relationship-type>
                <allow-content-type>base:folder</allow-content-type>
            </config>
        </input>
    </config>
</part>
