<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
  <display-name>HTML section</display-name>
  <config>
    <field-set name="HTML section">
      <label>HTML section</label>
      <items>
        <input type="TextLine" name="heading">
          <label>Heading</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input type="HtmlArea" name="html">
          <label>HTML</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input type="ImageSelector" name="image">
          <label>Image</label>
          <immutable>false</immutable>
          <indexed>false</indexed>
          <occurrences minimum="0" maximum="1"/>
          <config></config>
        </input>
        <input type="TextLine" name="buttonText">
          <label>Button Text</label>
          <immutable>false</immutable>
          <indexed>true</indexed>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input name="buttonContent" type="ContentSelector">
          <label>Button content</label>
          <immutable>false</immutable>
          <indexed>false</indexed>
          <occurrences minimum="0" maximum="1"/>
          <config>
            <relationship-type>system:reference</relationship-type>
          </config>
        </input>
      </items>
    </field-set>
  </config>
</part>
