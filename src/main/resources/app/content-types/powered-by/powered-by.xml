<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Powered by site</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="basic">
            <label>Powered by site</label>
            <items>
                <input type="TextLine" name="linkUrl">
                    <label>Link URL</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="1" maximum="1"/>
                </input>
                <input type="Tag" name="tag">
                    <label>Tags</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="0"/>
                </input>
            </items>
        </field-set>
        <field-set name="screenshots">
            <label>Screenshots</label>
            <items>
                <input type="ImageSelector" name="screenDesktop">
                    <label>Desktop</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="0" maximum="1"/>
                    <config></config>
                </input>
                <input type="ImageSelector" name="screenMobile">
                    <label>Mobile</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="0" maximum="1"/>
                    <config></config>
                </input>
            </items>
        </field-set>
    </form>
</content-type>