<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Product feature</display-name>
    <super-type>base:structured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="basic">
            <label>Product feature</label>
            <items>
                <input type="TextArea" name="description">
                    <label>Description</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>
                <item-set name="bullets">
                    <label>Bullets</label>
                    <items>
                        <input name="label" type="TextLine">
                            <label>Label</label>
                            <immutable>false</immutable>
                            <indexed>false</indexed>
                            <occurrences minimum="0" maximum="1"/>
                        </input>
                        <input name="value" type="TextLine">
                            <label>Value</label>
                            <immutable>false</immutable>
                            <indexed>false</indexed>
                            <occurrences minimum="0" maximum="1"/>
                        </input>
                    </items>
                    <immutable>false</immutable>
                    <occurrences minimum="0" maximum="10"/>
                </item-set>
                <input name="youtube-id" type="TextLine">
                    <label>Youtube ID</label>
                    <immutable>false</immutable>
                    <indexed>false</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>
                <item-set name="images">
                    <label>Image</label>
                    <items>
                        <input name="image" type="ImageSelector">
                            <label>Image</label>
                            <immutable>false</immutable>
                            <indexed>true</indexed>
                            <occurrences minimum="0" maximum="1"/>
                            <config></config>
                        </input>
                        <input name="image-caption" type="TextLine">
                            <label>Image caption</label>
                            <immutable>false</immutable>
                            <indexed>false</indexed>
                            <occurrences minimum="0" maximum="1"/>
                        </input>
                    </items>
                    <immutable>false</immutable>
                    <occurrences minimum="0" maximum="5"/>
                </item-set>

            </items>
        </field-set>
    </form>
</content-type>